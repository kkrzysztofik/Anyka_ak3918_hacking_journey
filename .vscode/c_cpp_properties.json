                "/opt/arm-anykav200-crosstool/usr/arm-anykav200-linux-uclibcgnueabi/include",
                "/opt/arm-anykav200-crosstool/usr/arm-anykav200-linux-uclibcgnueabi/sysroot/usr/include",
                "/opt/arm-anykav200-crosstool/usr/include",
                "/opt/arm-anykav200-crosstool/usr/include/c++/4.9.4",
                "/opt/arm-anykav200-crosstool/usr/include/c++/4.9.4/arm-anykav200-linux-uclibcgnueabi"
            ],
            "defines": [
                "ANYKA_PLATFORM=1",
